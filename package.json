{
  "name": "md2impress",
  "version": "0.0.4",
  "description": "a Markdown to Impress.js presentation generator",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/louisjordan/md2impress.git"
  },
  "author": "Louis Jordan <louisjordan@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "cli-dev": "nodemon bin/md2impress --input test/data/cli_input.md --output _temp/cli_output.html",
    "lint": "prettier --single-quote -l --write \"{src,test}/**/*.js\"",
    "pre-commit": "npm run build && npm run lint",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "jest": "@21.2.1",
    "prettier": "^1.8.2",
    "webpack": "@3.6.0"
  },
  "dependencies": {
    "commander": "^2.12.2",
    "marked": "^0.3.6"
  },
  "bugs": {
    "url": "https://github.com/louisjordan/md2impress/issues"
  },
  "homepage": "https://github.com/louisjordan/md2impress#readme",
  "bin": {
    "md2impress": "bin/md2impress.js"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": [
    "markdown",
    "impress",
    "presentation",
    "generator",
    "node",
    "browser",
    "cli"
